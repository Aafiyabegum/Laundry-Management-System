@model List<LaundryManagementSystem.Models.LoginModel>
@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}




<div class="span9">
    <div class="content">

        <div class="module" style="
    width: 1000px;
">
            <div class="module-head">
                <h3>User Details</h3>
            </div>
            <div class="module-body">
                <p>



                </p>
                <table class="table table-striped table-bordered table-condensed" id="ordertbl">
                    <thead>
                        <tr>
                            @*<th>User Id</th>*@
                            <th>Name</th>                   
                            <th>Username</th>
                            <th>Email Id</th>
                            <th>Type</th>
                            <th>Register Date</th>


                        </tr>
                    </thead>
                    @foreach (var item in Model)
                    {
                        <tbody>
                            <tr>
                                @*<td>@item.UserId</td>*@
                                <td>@item.FirstName @item.LastName</td>
                                <td>@item.UserName</td>
                                <td>@item.EmailId</td>
                                <td>@item.Type</td>                             
                                <td>@item.RegisterDate</td>

                            </tr>


                        </tbody>
                    }
                </table>

                <br />

            </div>
        </div>
    </div>
</div>
