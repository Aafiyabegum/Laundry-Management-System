@model List<LaundryManagementSystem.Models.ContactusModel>
@{
    ViewBag.Title = "Inbox";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}


<div class="span9">
    <div class="content">
        <div class="module" style="
    width: 1016px;
">
            <div class="module-head">
                <h3>Customer Inquiries</h3>
            </div>

       


                <table cellpadding="0" cellspacing="0" border="0" class=" table table-bordered table-striped	 display" width="100%">
                    <thead>
                        <tr>
                            <th>Inquiry No</th>
                            <th>Customer Name</th>
                            <th>Subject</th>
                            <th>Description</th>
                            <th>Email Address</th>
                            <th>Phone No</th>
                            <th>Posted date</th>

                        </tr>
                    </thead>
                    @foreach (var item in Model)
                    {
                        <tbody>
                            <tr class="odd gradeX">

                                <td>@item.ContatcId</td>
                                <td>
                                    @item.Name
                                </td>
                                <td>@item.Subject</td>
                                <td class="center">@item.Message</td>
                                <td>@item.EmailId</td>
                                <td>@item.PhoneNo</td>
                                <td>@item.Date.ToShortDateString()</td>

                            </tr>

                        </tbody>
                    }

                </table>

            </div>
        </div><!--/.module-->

        <br />

    </div><!--/.content-->
</div><!--/.s