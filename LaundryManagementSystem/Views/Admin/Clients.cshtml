@model List<LaundryManagementSystem.Models.LoginModel>
@{
    ViewBag.Title = "Clients";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}




<div class="span9">
    <div class="content">

        <div class="module" style="
    width: 1000px;
">
            <div class="module-head">
                <h3>Clients</h3>
            </div>
            <div class="module-body">
                <p>



                </p>
                <table class="table table-striped table-bordered table-condensed" id="ordertbl">
                    <thead>
                        <tr>
                            <th>User Id</th>
                            <th>Customer Name</th>
                            <th>User Name</th>
                            <th>Email Id</th>
                     

                        </tr>
                    </thead>
                    @foreach (var item in Model)
                    {
                        <tbody>
                            <tr>
                                <td>@item.UserId</td>
                                <td>@item.FirstName @item.LastName</td>
                                <td>@item.UserName</td>
                                <td>@item.EmailId</td>                               
                            </tr>


                        </tbody>
                    }
                </table>

                <br />

            </div>
        </div>
    </div>
</div>