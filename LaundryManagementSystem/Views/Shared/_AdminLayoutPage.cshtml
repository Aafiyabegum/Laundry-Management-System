<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link type="text/css" href="~/Content/admin/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link type="text/css" href="~/Content/admin/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet">
    <link type="text/css" href="~/Content/admin/css/theme.css" rel="stylesheet">
    <link type="text/css" href="~/Content/admin/images/icons/css/font-awesome.css" rel="stylesheet">
    <link type="text/css" href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600'
          rel='stylesheet'>



</head>
<body>

    <div>
        <div class="navbar navbar-fixed-top">
            <div class="navbar-inner" style="background-color: black;
">
                <div class="container">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".navbar-inverse-collapse">
                        <i class="icon-reorder shaded"></i>
                    </a><a class="brand" href="index.html" style="color: aliceblue;">Admin's Laundry System</a>

                    <!-- /.nav-collapse -->
                </div>
            </div>
            <!-- /navbar-inner -->
        </div>

        <!-- /navbar -->
        <div class="wrapper" style="height:800px; background-color: brown;">
            <div class="container">
                <div class="row" style="margin-left:-98px">
                    <div class="span3">
                        <div class="sidebar">
                            <ul class="widget widget-menu unstyled">
                                <li class="active">
                                    <a href="/Admin/Index">
                                        <i class="menu-icon icon-dashboard"></i>Dashboard
                                    </a>
                                </li>

                                <li>
                                    <a href="/Admin/Inbox">
                                        <i class="menu-icon icon-inbox"></i>Inbox <b class="label green pull-right">

                                        </b>
                                    </a>
                                </li>
                                <li>
                                    <a href="/Admin/AddPrice">
                                        <i class="menu-icon icon-inbox"></i>Add Price <b class="label green pull-right">

                                        </b>
                                    </a>
                                </li>
                                <li>
                                    <a href="/Admin/Orders">
                                        <i class="menu-icon icon-inbox"></i>Manage Orders<b class="label green pull-right">

                                        </b>
                                    </a>
                                </li>

                                <li>
                                    <a href="/Admin/ScheduleDelivery">
                                        <i class="menu-icon icon-inbox"></i> Manage Schedule<b class="label green pull-right">

                                        </b>
                                    </a>
                                </li>

                                <li>
                                    <a href="/Admin/Users">
                                        <i class="menu-icon icon-inbox"></i>Users<b class="label green pull-right">

                                        </b>
                                    </a>
                                </li>
                                <li>
                                    <a href="/Admin/Reports">
                                        <i class="menu-icon icon-inbox"></i>Reports<b class="label green pull-right">

                                        </b>
                                    </a>
                                </li>
                                <li>
                                    <a href="/Admin/Offers">
                                        <i class="menu-icon icon-inbox"></i>Offers and Latest News<b class="label green pull-right">

                                        </b>
                                    </a>
                                </li>
                            </ul>
                            <!--/.widget-nav-->
                            <!--/.widget-nav-->
                            <ul class="widget widget-menu unstyled">

                                <li><a href="/Admin/Logout"><i class="menu-icon icon-signout"></i>Logout </a></li>
                            </ul>
                        </div>
                        <!--/.sidebar-->
                    </div>


                    @RenderBody()


                </div>
            </div>
            <!--/.container-->
        </div>





        <!--/.wrapper-->
        <div class="footer" style="padding:0px">
            <div class="container">
            </div>
        </div>

    </div>





    <script src="~/Content/admin/scripts/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="~/Content/admin/scripts/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
    <script src="~/Content/admin/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Content/admin/scripts/flot/jquery.flot.js" type="text/javascript"></script>
    <script src="~/Content/admin/scripts/flot/jquery.flot.resize.js" type="text/javascript"></script>
    <script src="~/Content/admin/scripts/datatables/jquery.dataTables.js" type="text/javascript"></script>
    <script src="~/Content/admin/scripts/common.js" type="text/javascript"></script>

    @*<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>*@

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.4.1/jspdf.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf-autotable/2.3.5/jspdf.plugin.autotable.min.js"></script>
    <script src="~/Content/admin/scripts/tableHTMLExport.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
